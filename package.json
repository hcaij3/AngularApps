{
  "name": "angular-apps",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start demo": "ng run demo:serve --port 3000 -o",
    "start master-file": "ng serve --project=masterFile --port 3001 -o",
    "start md-co": "ng serve --project=md-co --port 5002 -o",
    "start common": "ng serve --project=common --port 5003 -o",
    "prod-masterFile-build": "npm run prod-masterFile-build-en & npm run prod-masterFile-build-fr",
    "prod-masterFile-build-en": "ng run masterFile:build:production-en & npm run updateBuild --language=en",
    "prod-masterFile-build-fr": "ng run masterFile:build:production-fr & npm run updateBuild --language=fr",
    "updateBuild": "node ./projects/master-file/update.build.js",
    "prod-md-co-build": "ng run md-co:build:production-en"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/localize": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ng-bootstrap/ng-bootstrap": "^14.0.1",
    "@ng-select/ng-select": "^10.0.3",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "file-saver": "^2.0.5",
    "ng2-select": "^2.0.0",
    "replace-in-file": "^6.3.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "util": "^0.12.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular/cli": "~15.0.5",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.0",
    "typescript": "~4.8.2"
  }
}
