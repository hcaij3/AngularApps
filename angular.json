{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@hpfb/sdk": {
      "projectType": "library",
      "root": "projects/hpfb/sdk",
      "sourceRoot": "projects/hpfb/sdk",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/hpfb/sdk/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/hpfb/sdk/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/hpfb/sdk/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/hpfb/sdk/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "md-co": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/md-co",
      "sourceRoot": "projects/md-co/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/md-co",
            "index": "projects/hpfb/sdk/src/index-en.html",
            "main": "projects/md-co/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/md-co/tsconfig.app.json",
            "assets": [
              "projects/md-co/src/favicon.ico",
              "projects/md-co/src/assets"
            ],
            "styles": [
              "projects/md-co/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production-en": {
              "outputPath": "dist/md-co/en",
              "index": "projects/hpfb/sdk/src/index-en.html",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/md-co/src/environments/environment.ts",
                  "with": "projects/md-co/src/environments/environment.prod-en.ts"
                }
              ]
            },
            "production-fr": {
              "outputPath": "dist/md-co/fr",
              "index": "projects/hpfb/sdk/src/index-fr.html",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/md-co/src/environments/environment.ts",
                  "with": "projects/md-co/src/environments/environment.prod-fr.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "md-co:build:production"
            },
            "development": {
              "browserTarget": "md-co:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "md-co:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/md-co/tsconfig.spec.json",
            "assets": [
              "projects/md-co/src/favicon.ico",
              "projects/md-co/src/assets"
            ],
            "styles": [
              "projects/md-co/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "md-co-internal": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/md-co",
      "sourceRoot": "projects/md-co/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/md-co-internal",
            "index": "projects/hpfb/sdk/src/index.html", 
            "main": "projects/md-co/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/md-co/tsconfig.app.json",
            "assets": [
              "projects/md-co/src/favicon.ico",
              "projects/md-co/src/assets"
            ],
            "styles": [
              "projects/md-co/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production-en": {
              "outputPath": "dist/md-co-internal/en",
              "index": "projects/hpfb/sdk/src/index-en.html",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/md-co/src/environments/environment.ts",
                  "with": "projects/md-co/src/environments/environment.prod.internal-en.ts"
                }
              ]
            },
            "production-fr": {
              "outputPath": "dist/md-co-internal/fr",
              "index": "projects/hpfb/sdk/src/index-fr.html",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/md-co/src/environments/environment.ts",
                  "with": "projects/md-co/src/environments/environment.prod.internal-fr.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "md-co-internal:build:production"
            },
            "development": {
              "browserTarget": "md-co-internal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "md-co-internal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/md-co/tsconfig.spec.json",
            "assets": [
              "projects/md-co/src/favicon.ico",
              "projects/md-co/src/assets"
            ],
            "styles": [
              "projects/md-co/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
